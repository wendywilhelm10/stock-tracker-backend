{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/stockWatch.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "stock": {
        "type": "string",
        "minLength": 2
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "stock"
    ]
}